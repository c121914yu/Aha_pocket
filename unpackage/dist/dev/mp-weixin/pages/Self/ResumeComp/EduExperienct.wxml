<view class="card" style="{{'height:'+(isEduExperience?'auto':'110rpx')+';'}}"><text class="h3">教育经历</text><block wx:for="{{eduExperience}}" wx:for-item="edu" wx:for-index="index" wx:key="index"><view class="list-itme"><view class="item degree"><text>学历</text><s-picker bind:input="__e" vue-id="{{'10b291d0-1-'+index}}" placeholder="本科,硕士" startIndex="3" range="{{Degress}}" value="{{edu.degree}}" data-event-opts="{{[['^input',[['__set_model',['$0','degree','$event',[]],[[['eduExperience','',index]]]]]]]}}" bind:__l="__l"></s-picker></view><view class="item school"><text>学校</text><search-input bind:input="__e" vue-id="{{'10b291d0-2-'+index}}" value="{{edu.school}}" data-event-opts="{{[['^input',[['__set_model',['$0','school','$event',[]],[[['eduExperience','',index]]]]]]]}}" bind:__l="__l"></search-input></view><view class="item specialty"><text>专业</text><input class="input" type="text" data-event-opts="{{[['input',[['__set_model',['$0','specialty','$event',[]],[[['eduExperience','',index]]]]]]]}}" value="{{edu.specialty}}" bindinput="__e"/></view><view class="item degree"><text>成绩排名</text><s-picker bind:input="__e" vue-id="{{'10b291d0-3-'+index}}" range="{{Grades}}" value="{{edu.grade}}" data-event-opts="{{[['^input',[['__set_model',['$0','grade','$event',[]],[[['eduExperience','',index]]]]]]]}}" bind:__l="__l"></s-picker></view><view class="item edu-time"><text>在校时间</text><data-picker bind:input="__e" vue-id="{{'10b291d0-4-'+index}}" placeholder="2018-9" startTime="2010-1" endTime="2030-12" value="{{edu.startTime}}" data-event-opts="{{[['^input',[['__set_model',['$0','startTime','$event',[]],[[['eduExperience','',index]]]]]]]}}" bind:__l="__l"></data-picker><view>-</view><data-picker bind:input="__e" vue-id="{{'10b291d0-5-'+index}}" placeholder="2018-9" startTime="2010-1" endTime="2030-12" value="{{edu.endTime}}" data-event-opts="{{[['^input',[['__set_model',['$0','endTime','$event',[]],[[['eduExperience','',index]]]]]]]}}" bind:__l="__l"></data-picker></view><icon class="remove" type="clear" size="22" color="#e86452" data-event-opts="{{[['tap',[['removeEdu',[index]]]]]}}" bindtap="__e"></icon></view></block><button data-event-opts="{{[['tap',[['addEdu',['$event']]]]]}}" class="add-btn" bindtap="__e">+添加教育经历</button></view>
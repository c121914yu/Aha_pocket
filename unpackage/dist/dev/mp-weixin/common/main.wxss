page {
  --nav-color: #f5c789;
  --origin1: #956134;
  --origin2: #f8b86b;
  --origin3: #f8d99f;
  --origin4: #feedcb;
  --black: #3e3a39;
  --gray1: #727171;
  --gray2: #9fa0a0;
  --white1: #efefef;
  --white2: #fff;
  --bg-linear: linear-gradient(#f8b86b, #956134);
  --shadow-beside: 0 5px 2px #efefef;
  --shadow1: 1px 1px 15px rgba(144,140,139,0.65);
  --shadow2: 3px 2px 8px rgba(144,140,139,0.65);
  --font-dark: #595757;
  --gray: #9fa0a0;
  --gray-font: #b5b5b6;
  --border1: 1px solid #9fa0a0;
  --speed-slide: 0.8s ease-in-out;
  --speed-hover: 0.5s;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
}
page {
  margin: 0;
  padding: 0s;
  font-size: 30rpx;
  line-height: 1.5;
  color: var(--black);
  letter-spacing: 2rpx;
}
input {
  font-size: 30rpx;
  padding: 10px 20px 10px 10px;
}
button {
  border: none;
  -webkit-border-radius: 12px;
          border-radius: 12px;
  font-size: 26rpx;
  padding: 5px;
  background-color: var(--origin2);
  color: #fff;
}
button::after {
  border: none;
}
button:active {
  -webkit-transform: scale(0.98);
          transform: scale(0.98);
}
/* hover样式 */
.button-hover {
  background-color: var(--origin2);
  color: #fff;
}
navigator {
  display: inline-block;
}
/* 默认placeholder样式 */
.input-placeholder {
  font-size: 24rpx;
  color: var(--gray2);
  letter-spacing: 0;
}
.hoverScale {
  -webkit-transform: scale(0.98);
          transform: scale(0.98);
}
.small {
  font-size: 26rpx;
}
.strong {
  font-weight: 700;
}
.center {
  text-align: center;
  margin: auto;
}
.h3 {
  font-size: 32rpx;
  font-weight: 700;
}


page {
  --nav-color: #f5c789;
  --bg-color1: #faf4eb;
  --bg-color2: #fffef7;
  --bg-color3: linear-gradient(#f7daaf, #f4c6b0);
  --button-bg: #f9c269;
  --origin-font: #f8b62d;
  --origin1: #f5c789;
  --origin2: #feedcb;
  --origin3: #f8d99f;
  --origin4: #f8b86b;
  --origin5: #f18f5a;
  --shadow1: 1px 1px 15px rgba(144,140,139,0.65);
  --shadow2: 3px 2px 8px rgba(144,140,139,0.65);
  --font-dark: #595757;
  --gray: #9fa0a0;
  --gray-font: #b5b5b6;
  --speed-slide: 0.8s ease-in-out;
  --speed-hover: 0.5s;
  margin: 0;
  padding: 0s;
  font-size: 32rpx;
  line-height: 1.5;
  color: var(--font-dark);
  letter-spacing: 2rpx;
}
view {
  box-sizing: border-box;
}
input {
  padding: 10px 20px 10px 10px;
}
button {
  border: none;
  border-radius: 12px;
}
button::after {
  border: none;
}
button:active {
  -webkit-transform: scale(0.98);
          transform: scale(0.98);
}
navigator {
  display: inline-block;
}
.placeholderStyle {
  font-size: 26rpx;
  color: var(--gray-font);
}
.small {
  font-size: 26rpx;
}
.strong {
  font-weight: 600;
}
.center {
  text-align: center;
  margin: auto;
}
.h3 {
  font-size: 36rpx;
  font-weight: 600;
}

@font-face {
  font-family: 'iconfont';  /* project id 2102312 */
  src: url('//at.alicdn.com/t/font_2102312_gfg8ecasxb5.eot');
  src: url('//at.alicdn.com/t/font_2102312_gfg8ecasxb5.eot?#iefix') format('embedded-opentype'),
  url('//at.alicdn.com/t/font_2102312_gfg8ecasxb5.woff2') format('woff2'),
  url('//at.alicdn.com/t/font_2102312_gfg8ecasxb5.woff') format('woff'),
  url('//at.alicdn.com/t/font_2102312_gfg8ecasxb5.ttf') format('truetype'),
  url('//at.alicdn.com/t/font_2102312_gfg8ecasxb5.svg#iconfont') format('svg');
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

